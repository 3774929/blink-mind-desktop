{
  "name": "blink-mind-desktop",
  "productName": "BlinkMind",
  "version": "0.1.0",
  "private": true,
  "author": "awehook",
  "dependencies": {
    "@blink-mind/core": "0.0.20-beta.9",
    "@blink-mind/icons": "0.0.20-beta.9",
    "@blink-mind/plugin-json-serializer": "0.0.20-beta.9",
    "@blink-mind/plugin-rich-text-editor": "0.0.20-beta.9",
    "@blink-mind/plugin-theme-selector": "0.0.20-beta.9",
    "@blink-mind/plugin-topology-diagram": "0.0.20-beta.9",
    "@blink-mind/plugins": "0.0.20-beta.9",
    "@blink-mind/renderer-react": "0.0.20-beta.9",
    "@blueprintjs/core": "^3.19.1",
    "@blueprintjs/icons": "^3.13.0",
    "classnames": "^2.2.6",
    "electron-is-dev": "^1.1.0",
    "electron-store": "^5.1.0",
    "electron-window-state": "^5.0.3",
    "i18next": "^19.0.3",
    "i18next-node-fs-backend": "^2.1.3",
    "immutable": "^4.0.0-rc.12",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "styled-components": "^4.4.0",
    "universal-analytics": "^0.4.20"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/helper-create-regexp-features-plugin": "^7.7.4",
    "@svgr/webpack": "4.1.0",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.1.2",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "babel-plugin-named-asset-import": "^0.3.1",
    "babel-preset-react-app": "^7.0.2",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "commander": "^4.1.0",
    "cross-env": "^6.0.3",
    "css-loader": "1.0.0",
    "del": "^5.1.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "drop-console-webpack-plugin": "^3.0.2",
    "electron": "^7.1.9",
    "electron-builder": "^22.2.0",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "5.12.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "2.0.0",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.2",
    "jest-resolve": "23.6.0",
    "jest-watch-typeahead": "^0.2.1",
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.5.0",
    "postcss-safe-parser": "4.0.1",
    "prettier": "^1.19.1",
    "react-app-polyfill": "^0.2.2",
    "react-dev-utils": "^8.0.0",
    "resolve": "1.10.0",
    "rewire": "^4.0.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.2",
    "typescript": "^3.3.1",
    "url-loader": "1.1.2",
    "vsce": "^1.69.0",
    "vscode": "^1.1.6",
    "webpack": "4.28.3",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-dev-server": "3.1.14",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "start-r": "cross-env NODE_ENV=development node --inspect=5858 scripts/start.js",
    "start-m": "cross-env NODE_ENV=development ./node_modules/.bin/electron --inspect=5859 ./src/main",
    "dev": "node .electron/dev-runner.js",
    "dbg": "node .electron/dev-runner.js --dbg",
    "up-blink": "node scripts/blink.js -u",
    "link-blink": "node scripts/blink.js --link",
    "unlink-blink": "node scripts/blink.js --unlink",
    "package": "electron-builder build --publish never",
    "pack:r": "cross-env GENERATE_SOURCEMAP=false yarn build",
    "pack:m": "node ./.electron/build.js",
    "dist": "yarn pack:r && yarn pack:m && yarn package",
    "run:mac": "./dist/mac/BlinkMind.app/Contents/MacOS/BlinkMind"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "main": "./build/electron/main.js",
  "build": {
    "productName": "BlinkMind",
    "appId": "org.awehook.blinkminddesktop",
    "files": [
      "build/electron/**/*",
      "build/renderer/**/*",
      "index.html"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "mac": {
      "target": [
        "zip"
      ]
    },
    "win": {
      "target": [
        "zip"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "output": "dist"
    }
  }
}
